{
	'presets': [
		['@babel/preset-env', {
			'targets': {
				'browsers': ['ie >= 11']
			},
			'modules': false
		}],

		['@babel/preset-react', {}]
	],

	plugins: [
		'@babel/plugin-syntax-dynamic-import',
		'@babel/plugin-proposal-decorators',
		['@babel/plugin-proposal-class-properties', { "loose" : true }],
		'react-hot-loader/babel'
	]
}

